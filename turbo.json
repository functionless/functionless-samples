{
  "npmClient": "yarn",
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "build/**",
        "dist/**",
        "lib/**",
        "storybook-static/**"
      ]
    },
    "compile": {
      "dependsOn": [
        "^compile"
      ],
      "outputs": [
        "build/**",
        "dist/**",
        "lib/**",
        "storybook-static/**"
      ]
    },
    "test": {
      "dependsOn": [
        "^compile"
      ],
      "outputs": [
        "coverage/**",
        "test-reports/**"
      ]
    },
    "eslint": {
      "outputs": []
    },
    "watch": {
      "cache": false
    }
  },
  "//": "~~ Generated by projen. To modify, edit .projenrc.js and run \"npx projen\"."
}
